{
  "url": {
    "base": "https://lms.iiitb.ac.in/moodle",
    "login": "/login/index.php",
    "home": "/my"
  },
  "flags": {
  	"allowInsecure": true
  },
  "status": {
    "loggedOut": "You are not logged in.",
    "loggedIn": "You are logged in as "

  },
  "params": {
    "onlineTimeout": 3000
  },
  "class": {
  	"headerMenu": ".headermenu",
  	"userStatus": ".logininfo"
  },
  "id": {
    "course": {
      "forum": "#section-0",
      "material": "#section-1",
      "activities": "#section-2"
    }
  },
  "selector": {
    "user": {
      "privateFiles": {
        "selector": "div[id^=\"private_files_tree_\"]",
        "link": "a @href",
        "name": "a"
      },
      "courseList": {
        "selector": "#page-content",
        "link": ".corner-box .content .course_list h2 @href",
        "name": ".corner-box .content .course_list h2"
      }
    }
  }
}